/// @description Typically custom values
// If you want to use this for your own engine (like any other assets)- please ask ;-)

// Enemy to spawn
enemy_id = obj_goomba;

// Ready to respawn
primed = true;

// General porting IDs
color = -1;
palette = -1;

// Despawn enemy (used so that when Yoshi has an enemy in his mouth he won't despawn - don't modify)
despawn_enemy = true;

// Is the spawner currently inactive due to an enemy's full death (to be re-enabled on warps, etc)
inactive = false;

// Static
my_enemy = noone;

// Added in from a line
lined = false;